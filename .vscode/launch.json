{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "args": ["local.json"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\res",
      "environment": [ ],
      "externalConsole": true,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": false 
        }
      ]
    }
  ]
}