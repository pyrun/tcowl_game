{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Project",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}\\bin\\windebug32\\TheCommemorationOfWhiteLight.exe",
      "args": ["server"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}\\res",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build Project"
    }
  ]
}